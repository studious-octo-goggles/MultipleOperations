<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.prateek.multipleoperations">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/e"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NumericOperation"
            android:label="NumericOperations" />
        <activity
            android:name=".OtherOperation"
            android:label="OtherOperations" />
        <activity
            android:name=".StringOperation"
            android:label="StringOperations" />
        <activity android:name=".lcmgcd"></activity>
    </application>

</manifest>